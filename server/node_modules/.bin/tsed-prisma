#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jabriel/dev/FYP/calitdone/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/jabriel/dev/FYP/calitdone/server/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/ts-node" ]; then
  exec "$basedir/ts-node"  "$basedir/../@tsed/prisma/lib/cjs/generator.js" "$@"
else
  exec ts-node  "$basedir/../@tsed/prisma/lib/cjs/generator.js" "$@"
fi
